@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;

    MarketingTools.Models.CompanyInfo company = ViewBag.CompanyInfo;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Cube - Bootstrap Admin Template</title>

    <!-- bootstrap -->
    <link rel="stylesheet" type="text/css" href="/css/cube2/bootstrap/bootstrap.min.css" />

    <!-- libraries -->
    <link rel="stylesheet" type="text/css" href="/css/cube2/libs/font-awesome.css" />
    <link rel="stylesheet" type="text/css" href="/css/cube2/libs/nanoscroller.css" />
	
    <!-- global styles -->
    <link rel="stylesheet" type="text/css" href="/css/cube2/compiled/theme_styles.css" />
	<link rel="stylesheet" type="text/css" href="/App_Plugins/IconPicker/css/umbraco-icons.css" />
	<link rel="stylesheet" type="text/css" href="/App_Plugins/IconPicker/css/addon-icons.css" />

    <!-- this page specific styles -->
    @RenderSection("pageCss", false)

    <!-- Favicon -->
    <link type="image/x-icon" href="/favicon.png" rel="shortcut icon" />
    <!-- google font libraries -->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,600,700,300' rel='stylesheet' type='text/css'>
    <!--[if lt IE 9]>
        <script src="/scripts/html5shiv.js"></script>
        <script src="/scripts/respond.min.js"></script>
    <![endif]-->

    @if (company != null)
    {
        <script type="text/javascript">
            sessionStorage.setItem('clientName', '@company.CompanyName');
            sessionStorage.setItem('clientId', '@company.CompanyId');
            sessionStorage.setItem('clientShortCode', '@company.CompanyShortCode');
            sessionStorage.setItem('clientLogoUrl', '@company.CompanyLogoUrl');
        </script>
    }

	@RenderSection("topScripts", false)
</head>
<body  class="theme-whbl">
    <div id="theme-wrapper">
        <header class="navbar" id="header-navbar">
            <div class="container">
                <a href="/" id="logo" class="navbar-brand">
                    <img src="~/Themes/cube2/img/samples/buckaz-mt-logo.png" alt="" class="normal-logo logo-white" />
                    <img src="/Themes/cube2/img/logo-black.png" alt="" class="normal-logo logo-black" />
                    <img src="/Themes/cube2/img/logo-small.png" alt="" class="small-logo hidden-xs hidden-sm hidden" />
                </a>

                <div class="clearfix">
                    <button class="navbar-toggle" data-target=".navbar-ex1-collapse" data-toggle="collapse" type="button">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="fa fa-bars"></span>
                    </button>

                    <div class="nav-no-collapse navbar-left pull-left hidden-sm hidden-xs">
                        <ul class="nav navbar-nav pull-left">
                            <li>
                                <a class="btn" id="make-small-nav">
                                    <i class="fa fa-bars"></i>
                                </a>
                            </li>
                        </ul>
                    </div>

                    <div class="nav-no-collapse pull-right" id="header-nav">
                        <ul class="nav navbar-nav pull-right">
                            <li class="dropdown profile-dropdown">
                                <a href="#">
                                    <img src="/Themes/cube2/img/samples/scarlet-159.png" alt="" />
                                    <span class="hidden-xs">Sample User</span>
                                </a>
                            </li>
                            <li class="hidden-xxs">
                                <a class="btn">
                                    <i class="fa fa-power-off"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
        <div id="page-wrapper" class="container">
            <div class="row">
                <div id="nav-col">
                    <section id="col-left" class="col-left-nano">
                        <div id="col-left-inner" class="col-left-nano-content">
                            <div id="user-left-box" class="clearfix hidden-sm hidden-xs dropdown profile2-dropdown">
                                <img src="~/Themes/cube2/img/samples/phps/logo.jpg" />
                                <div class="user-box">
                                    <span class="name">
                                        <a href="#" class="dropdown-toggle" style="font-size: .8em;">
                                            Phoenix Health Plans &amp; Allegian Health Plans
                                        </a>
                                    </span>
                                </div>
                            </div>
                            <div class="collapse navbar-collapse navbar-ex1-collapse" id="sidebar-nav">
								@Umbraco.RenderMacro("MainNavigation", new {startNodeId="1059"})
                            </div>
                        </div>
                    </section>
                    <div id="nav-col-submenu"></div>
                </div>
                <div id="content-wrapper">
                    <div class="row">
                        <div class="col-lg-12">
                        @RenderBody()
                            
                        </div>
                    </div>

                    <footer id="footer-bar" class="row">
                        <p id="footer-copyright" class="col-xs-12">
                            Powered by Cube Theme.
                        </p>
                    </footer>
                </div>
            </div>
        </div>
    </div>


    <!-- global scripts -->
    <script src="/scripts/jquery.js"></script>
    <script src="/scripts/bootstrap.js"></script>
    <script src="/scripts/jquery.nanoscroller.min.js"></script>

    <!-- this page specific scripts -->
	@RenderSection("pageScripts", false)

    <!-- theme scripts -->
    <script src="/scripts/scripts.js"></script>
    <script src="/scripts/pace.min.js"></script>

    <!-- this page specific inline scripts -->

</body>
</html>
